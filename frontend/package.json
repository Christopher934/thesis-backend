{
  "name": "rsud-anugerah-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=4096' NEXT_TELEMETRY_DISABLED=1 next dev -p 3000 --turbo",
    "dev:ultra": "NODE_OPTIONS='--max-old-space-size=2048' NEXT_TELEMETRY_DISABLED=1 DISABLE_SOURCE_MAPS=true NEXT_PRIVATE_SKIP_VALIDATION=1 next dev -p 3000",
    "dev:fast": "NODE_OPTIONS='--max-old-space-size=4096' NEXT_TELEMETRY_DISABLED=1 NEXT_PRIVATE_SKIP_VALIDATION=1 next dev -p 3000 --turbo",
    "dev:stable": "NODE_OPTIONS='--max-old-space-size=2048' next dev -p 3000",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "build:docker": "NODE_ENV=production NODE_OPTIONS='--max-old-space-size=4096' next build",
    "analyze": "ANALYZE=true npm run build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out || true",
    "perf:build": "NODE_ENV=production npm run build",
    "perf:start": "NODE_ENV=production npm start",
    "lighthouse": "lighthouse http://localhost:3000 --output=html --output-path=lighthouse-report.html"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.20",
    "@types/react-big-calendar": "^1.16.1",
    "axios": "^1.9.0",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.16.0",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.516.0",
    "moment": "^2.30.1",
    "next": "^15.3.3",
    "react": "^18",
    "react-big-calendar": "^1.18.0",
    "react-calendar": "^5.1.0",
    "react-dom": "^18",
    "react-hook-form": "^7.56.4",
    "react-hot-toast": "^2.5.2",
    "recharts": "^2.15.3",
    "socket.io-client": "^4.8.1",
    "victory-vendor": "^37.3.6",
    "xmlhttprequest-ssl": "^4.0.0",
    "zod": "^3.25.30"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.3.5",
    "@types/axios": "^0.9.36",
    "@types/jest": "^29.5.14",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.21",
    "eslint": "^8",
    "eslint-config-next": "^15.3.3",
    "jest": "^29.7.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.3.4",
    "typescript": "^5"
  }
}
